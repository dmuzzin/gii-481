{
    "name": "Describ-o-tron 9000",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Transcribes images for vision impaired users",
    "icons": {
	"38" : "icon-38.png",
	"128": "icon-128.png"
    },
    "browser_action": {
	"default_icon": "icon-38.png"
    },
    "background": {
	"persistent": true,
	"scripts": ["background.js", "jquery.min.js"]
    },
	"permissions": [
		"<all_urls>",
		"storage",
		"tabs"
	],
    "content_scripts": [
	{
	    "matches": ["<all_urls>"],
	    "js": ["gii-dev-keys/keys.js", "jquery.min.js", "replace_alt.js", "image_ident.js"],
	    "run_at": "document_end"
	}
    ]
}
