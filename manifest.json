{
    "name": "Describ-o-tron 9000",
    "manifest_version": 2,
    "version": "0.01",
    "description": "Transcribes images for vision impaired users",
    "icons": { "128": "icon.png" },
    "background": {
	"persistent": false,
	"scripts": ["background.js"]
    },
    "content_scripts": [
	{
	    "matches": ["<all_urls>"],
	    "js": ["image_ident.js"],
	    "run_at": "document_end"
	}
    ]
}
